{
    "name": "ibotpeaches/leafapp_infinite",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "artesaos/seotools": "^1.1",
        "bensampo/laravel-enum": "^6.3",
        "guzzlehttp/guzzle": "^7.4",
        "jaybizzle/laravel-crawler-detect": "^1.3",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^v5.15.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "league/csv": "^9.8",
        "livewire/livewire": "^3.0",
        "sentry/sentry-laravel": "^4.0",
        "spatie/laravel-markdown": "^2.2",
        "spatie/laravel-sitemap": "^7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "@php artisan test --coverage --min=100 --coverage-html=output"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
